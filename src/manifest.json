{
  "name": "QR Code",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "translate localhost url into ip url, and generate QR Code",
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}