{
  "name": "MyQRCode",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "QRCode Generator",
  "icons": {
    "128": "images/logo_128.png",
    "16": "images/logo_16.png",
    "48": "images/logo_48.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}